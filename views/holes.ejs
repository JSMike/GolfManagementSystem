<div>
  <h3 class="text-center">Holes</h3>
  <div class="container">
    <div class="row">
      <ul ng-init="Holes()">
        <li ng-repeat="hole in db.holes track by $index">
          <p><span class="col-xs-2">HOLE_ID:</span> <input class="col-xs-10" type='text' ng-model='hole.HOLE_ID' disabled></p>
          <p><span class="col-xs-2">COURSE_ID:</span> <input class="col-xs-10" type='text' ng-model='hole.COURSE_ID' disabled></p>
          <p><span class="col-xs-2">HOLENUM:</span> <input class="col-xs-10" type='text' ng-model='hole.HOLENUM' disabled></p>
          <p><span class="col-xs-2">PAR:</span> <input class="col-xs-10" type='text' ng-model='hole.PAR' disabled></p>
          <p><span class="col-xs-2">YARDS:</span> <input class="col-xs-10" type='text' ng-model='hole.YARDS' disabled></p>
          <p><span class="col-xs-2">HANDICAP:</span> <input class="col-xs-10" type='text' ng-model='hole.HANDICAP' disabled></p>
        <li>
      </ul>
    </div>
  </div>
</div>
