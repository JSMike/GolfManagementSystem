<div>
  <h3 class="text-center">Teams</h3>
  <ul ng-init="get.Teams()">
    <li ng-repeat="team in db.teams track by $index">
      <p>TEAM_ID: <input type='text' ng-model='team.TEAM_ID' disabled></p>
      <p>PLAYER1: <input type='text' ng-model='team.PLAYER1' disabled></p>
      <p>PLAYER2: <input type='text' ng-model='team.PLAYER2' disabled></p>
    <li>
  </ul>
</div>
